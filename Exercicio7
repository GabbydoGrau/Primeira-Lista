var caractere = prompt("Digite um caractere:");
var inteiro = parseInt(prompt("Digite um número inteiro:"));
var float = parseFloat(prompt("Digite um número de ponto float:"));

console.log(caractere, inteiro, float);

console.log(caractere + "\t" + inteiro + "\t" + float);

console.log(caractere);
console.log(inteiro);
console.log(float);
